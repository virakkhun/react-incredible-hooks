{
	"name": "urh-react-hooks",
	"version": "1.2.16",
	"private": false,
	"author": "Virak Khun",
	"license": "MIT",
	"description": "The simple react hooks.",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"type": "module",
	"typings": "./dist/index.d.ts",
	"files": [
		"./dist/index.mjs",
		"./dist/index.cjs",
		"./dist/index.d.ts",
		"./README.md",
		"./CHANGELOG.md"
	],
	"scripts": {
		"rollup": "rollup --config",
		"clean": "rm -rf dist",
		"build": "yarn clean && yarn rollup",
		"version": "yarn changeset && yarn changeset version",
		"release": "yarn build && yarn changeset publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/virakkhun/urh-react-hooks.git"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./*": "./*"
	},
	"keywords": [
		"react",
		"react-dom",
		"react-hooks",
		"hooks",
		"dom",
		"utilities"
	],
	"bugs": {
		"url": "https://github.com/virakkhun/urh-react-hooks.git/issues"
	},
	"homepage": "https://github.com/virakkhun/urh-react-hooks.git#readme",
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
